doctype html
html(lang='en-us')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    title Innotech International, LLC
    link(rel='shortcut icon' href='img/favicon.ico')
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl' crossorigin='anonymous')
    link(rel='preconnect' href='https://fonts.gstatic.com')
    link(crossorigin='anonymous' integrity='' href='https://fonts.googleapis.com/css2?family=Arvo:wght@700&display=swap' rel='stylesheet')
    link(crossorigin='anonymous' integrity='' href='https://fonts.googleapis.com/css2?family=Lato:wght@700&display=swap' rel='stylesheet')
    link(rel='stylesheet' href='css/main.css' media='screen' title='no title' charset='utf-8')
  body
    main.container.container-main.lifted
      nav.navbar.navbar-default.navbar-dark.navbar-expand-lg
        .container-fluid
          // nav Brand
          a.navbar-brand(href='#')
            img(src='img/logo_nobg_white.png' alt='Innotech Logo')
          // nav Toggle Button
          button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarmenu' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
            span.navbar-toggler-icon
          // nav collapsable menu
          #navbarmenu.collapse.navbar-collapse
            ul.navbar-nav.me-auto.mb-2.mb-lg-0
              li.nav-item.main-nav-item
                a.nav-link(href='index.html') HOME
              li.nav-item.main-nav-item
                a.nav-link(href='mission.html') MISSION
              li.nav-item.main-nav-item
                a.nav-link(href='services.html') SERVICES
              li.nav-item.main-nav-item
                a.nav-link(href='links.html') LINKS
              li.nav-item.main-nav-item
                a.nav-link.active(href='clients.html') CLIENTS
              li.nav-item.main-nav-item
                a.nav-link(href='contact.html') CONTACT
      article.mt-1
        // Content goes here
