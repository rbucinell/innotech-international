doctype html
html(lang='en-us')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    title Innotech International, LLC
    link(rel='shortcut icon' href='img/favicon.ico')
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl' crossorigin='anonymous')
    link(rel='preconnect' href='https://fonts.gstatic.com')
    link(crossorigin='anonymous' integrity='' href='https://fonts.googleapis.com/css2?family=Arvo:wght@700&display=swap' rel='stylesheet')
    link(crossorigin='anonymous' integrity='' href='https://fonts.googleapis.com/css2?family=Lato:wght@700&display=swap' rel='stylesheet')
    link(rel='stylesheet' href='css/main.css' media='screen' title='no title' charset='utf-8')
  body
    main.container.container-main.lifted
      nav.navbar.navbar-default.navbar-dark.navbar-expand-lg
        .container-fluid
          // nav Brand
          a.navbar-brand(href='#')
            img(src='img/logo_nobg_white.png' alt='Innotech Logo')
          // nav Toggle Button
          button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarmenu' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
            span.navbar-toggler-icon
          // nav collapsable menu
          #navbarmenu.collapse.navbar-collapse
            ul.navbar-nav.me-auto.mb-2.mb-lg-0
              li.nav-item.main-nav-item
                a.nav-link(href='index.html') HOME
              li.nav-item.main-nav-item
                a.nav-link(href='mission.html') MISSION
              li.nav-item.main-nav-item
                a.nav-link(href='services.html') SERVICES
              li.nav-item.main-nav-item
                a.nav-link(href='links.html') LINKS
              li.nav-item.main-nav-item
                a.nav-link(href='clients.html') CLIENTS
              li.nav-item.main-nav-item
                a.nav-link.active(href='contact.html') CONTACT
      article.mt-1
        .card.bg-transparent.mb-2.border-0
          .card-header.pattern.lifted.p-3 Contact Us
        .row
          .col-md-7
            iframe(title='google maps image' src='https://www.google.com/maps/embed/v1/place?q=1063%20Merlin%20Dr%20Niskayuna%20NY%2012309&key=AIzaSyBFw0Qbyq9zTFTd-tUY6dZWTgaQzuU17R8' width='100%' height='315')
          .col-md-5
            form
              .form-group
                input.form-control(type='text' name='' value='' placeholder='Name')
              .form-group
                input.form-control(type='email' name='' value='' placeholder='E-mail')
              .form-group
                input.form-control(type='tel' name='' value='' placeholder='Phone')
              .form-group
                textarea.form-control(name='' rows='3' placeholder='Message')
              button.btn.btn-primary.m-2(type='submit' name='button')  Submit 
    // JavaScript Bundle with Popper
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js' integrity='sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0' crossorigin='anonymous')
